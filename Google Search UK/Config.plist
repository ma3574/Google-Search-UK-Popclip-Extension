<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>Version</key>
	<integer>1</integer>
	<key>Extension Name</key>
	<string>Google Search UK</string>
	<key>Extension Identifier</key>
	<string>uk.co.mahmad.popclip.extension.google-search-uk</string>
	<key>Extension Description</key>
	<string>Open search results using an encrypted version of Google UK using the selected text.</string>
	<key>Actions</key>
	<array>
		<dict>
			<key>URL</key>
			<string>https://www.google.co.uk/#q={popclip text}</string>
			<key>Image File</key>
			<string>SearchIcon.png</string>
			<key>Title</key>
			<string>Google Search UK</string>
		</dict>
	</array>
</dict>
</plist>
